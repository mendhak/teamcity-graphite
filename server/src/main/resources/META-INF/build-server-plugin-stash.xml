        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >


  <bean class="mendhak.teamcity.stash.ui.GraphiteBuildFeature"/>
  <bean class="mendhak.teamcity.stash.ui.SettingsPageController"/>

  <bean class="mendhak.teamcity.stash.BuildStatusListener"/>
