<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >


  <bean class="mendhak.teamcity.graphite.ui.GraphiteBuildFeature"/>
  <bean class="mendhak.teamcity.graphite.ui.SettingsPageController"/>

  <bean class="mendhak.teamcity.graphite.BuildStatusListener"/>
  <bean class="mendhak.teamcity.graphite.ChangeStatusUpdater"/>

  <bean class="mendhak.teamcity.graphite.api.GraphiteClient"/>

  <bean class="mendhak.teamcity.graphite.GraphiteBuildParametersProvider"/>

</beans>
